{
  "parser": "babel-eslint",
  "extends": ["@kael89/js", "@kael89/jest"],
  "plugins": ["cypress"],
  "env": {
    "browser": true,
    "cypress/globals": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "import/no-default-export": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": ["cypress/**/*"] }]
  }
}
