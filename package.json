{
  "name": "kill-the-king",
  "version": "1.0.0",
  "description": "A web app that calculates forced checkmates in a given chess position",
  "homepage": "https://killtheking.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/kael89/kill-the-king"
  },
  "author": "Kostas Karvounis",
  "license": "GPL-3.0-or-later",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint \"**/*.{js,jsx}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix & prettier \"**/*.{json,md}\" --write --ignore-path .gitignore",
    "test": "react-scripts test",
    "test:e2e": "npm run start & wait-on http://localhost:3000 && cypress run",
    "test:all": "CI=true npm test; npm run test:e2e",
    "ts:check": "tsc -p jsconfig.json",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "cross-fetch": "^3.0.1",
    "lodash": "^4.17.14",
    "prism-react-renderer": "^0.1.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0"
  },
  "devDependencies": {
    "@kael89/eslint-config-jest": "^0.1.0",
    "@kael89/eslint-config-js": "^0.1.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.7.1",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "cypress": "^3.4.0",
    "eslint": "^8.14.0",
    "eslint-plugin-cypress": "^2.2.1",
    "prettier": "^2.6.2"
  }
}
